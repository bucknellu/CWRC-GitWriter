{
  "name": "cwrc-git-writer",
  "version": "0.0.0",
  "description": "xml editing and rdf annotation using github as document store",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwrc/CWRC-GitWriter.git"
  },
  "keywords": [
    "xml",
    "rdf",
    "editor",
    "annotation"
  ],
  "contributors": [
    "Andrew MacDonald",
    "James Chartrand"
  ],
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/cwrc/CWRC-GitWriter/issues"
  },
  "homepage": "https://github.com/cwrc/CWRC-GitWriter#readme",
  "scripts": {
    "cm": "git-cz",
    "build": "rimraf build && npm run build-cwrc-writer-base && npm run copy-files && npm run webpack-prod",
    "webpack-prod": "webpack --mode production",
    "webpack-dev": "webpack --mode development --debug=true --display-error-details=true",
    "dev-server": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build-cwrc-writer-base": "cd \"node_modules/cwrc-writer-base\" && npm run build",
    "copy-files": "npm run copy-css && npm run copy-images && npm run copy-bootstrap-fonts",
    "copy-css": "copyfiles -u 3 \"node_modules/cwrc-writer-base/build/css/**/*\" build && copyfiles -u 3 \"node_modules/cwrc-git-dialogs/src/css/bootstrap.css\" build",
    "copy-images": "copyfiles -u 3 \"node_modules/cwrc-writer-base/src/img/**/*\" build/",
    "copy-bootstrap-fonts": "copyfiles -f \"node_modules/bootstrap/fonts/*\" build/fonts/",
    "stats": "webpack --profile --json > build/stats.json",
    "analyzer": "webpack-bundle-analyzer build/stats.json"
  },
  "dependencies": {
    "cwrc-git-dialogs": "github:cwrc/cwrc-git-dialogs#development",
    "cwrc-public-entity-dialogs": "^1.16.1",
    "cwrc-writer-base": "github:cwrc/CWRC-WriterBase#development",
    "dbpedia-entity-lookup": "^0.6.1",
    "geonames-entity-lookup": "^1.2.0",
    "getty-entity-lookup": "^1.5.0",
    "lgpn-entity-lookup": "^1.0.0",
    "viaf-entity-lookup": "^1.4.1",
    "wikidata-entity-lookup": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babelify": "^10.0.0",
    "commitizen": "^4.0.3",
    "copyfiles": "^1.2.0",
    "core-js": "^2.6.9",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mkdirp": "^0.5.1",
    "path-exists-cli": "^1.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
